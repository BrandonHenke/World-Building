\newcommand{\bibSetup}[1]{%
	\usepackage[
		backend=bibtex,
		style=phys,
		articletitle=false,
		biblabel=superscript,
		chaptertitle=false,
		pageranges=false,
		sorting=none,
		autocite=superscript,
		natbib=true
	]{biblatex}
	\addbibresource{#1}
    \gdef\BIBFILE{#1}
}


